{
  "name": "@iml/srcmap-reverse-server",
  "version": "1.0.1",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "dist/srcmap-reverse-server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node $npm_package_main",
    "prepublish": "old_pwd=$PWD; cd ../..; yarn run rollup -- -i $old_pwd/source/index.js -o $old_pwd/$npm_package_main"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "port": "8082"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/srcmap-reverse-server.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "@iml/srcmap-reverser": "^1.0.1"
  },
  "devDependencies": {
    "@iml/req": "3.0.4"
  }
}
