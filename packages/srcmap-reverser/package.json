{
  "name": "@iml/srcmap-reverser",
  "version": "0.0.0",
  "description": "Reverse a single line using a specified source map.",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublish": "old_pwd=$PWD; cd ../..; yarn run rollup -- -i $old_pwd/source/index.js -o $old_pwd/$npm_package_main"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/srcmap-reverser.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "highland": "3.0.0-beta.4",
    "source-map": "0.5.6"
  }
}
