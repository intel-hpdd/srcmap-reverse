{
  "name": "@mfl/srcmap-reverse",
  "version": "2.0.0",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "dist/bundle.js",
  "scripts": {
    "prepublish": "old_pwd=$PWD; cd ../..; yarn run rollup -- -i $old_pwd/source/index.js -o $old_pwd/$npm_package_main"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "port": "8082",
    "srcMapFile": "/usr/share/chroma-manager/ui-modules/node_modules/@iml/gui/dist/built*.map",
    "loggerPath": "/var/log/chroma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/srcmap-reverse.git"
  },
  "author": "MFL Team",
  "license": "MIT",
  "dependencies": {
    "@mfl/srcmap-reverser": "0.0.0"
  }
}
