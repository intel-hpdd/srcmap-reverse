{
  "name": "@iml/srcmap-reverse",
  "version": "2.0.0",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node $npm_package_main",
    "prepublish": "old_pwd=$PWD; cd ../..; yarn run rollup -- -i $old_pwd/source/index.js -o $old_pwd/$npm_package_main"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "port": "8082",
    "loggerPath": "/var/log/chroma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/srcmap-reverse.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "highland": "3.0.0-beta.4",
    "@iml/req": "3.0.4",
    "@iml/srcmap-reverser": "0.0.0"
  }
}
