{
  "name": "intel-srcmap-reverse",
  "version": "2.0.0",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "dist/source/index.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t",
    "eslint": "eslint ./",
    "postversion": "./bin/build.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/srcmap-reverse.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-strict-mode": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "eslint": "3.0.1",
    "eslint-plugin-flow-vars": "0.4.0",
    "eslint-plugin-flowtype": "2.3.0",
    "intel-flow-jasmine": "1.1.0",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "nyc": "6.6.1",
    "pre-commit": "1.1.3"
  },
  "dependencies": {
    "source-map": "0.5.6"
  }
}
