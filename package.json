{
  "name": "@iml/srcmap-reverse",
  "version": "3.0.0",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "dist/srcmap-reverse.js",
  "scripts": {
    "pretest": "yarn rollup",
    "test": "jest",
    "cover": "yarn run test -- --coverage",
    "flow": "flow",
    "eslint": "eslint ./",
    "rollup": "rollup -c rollup-config.js -i source/index.js -o $npm_package_main",
    "postversion": "rm -rf ./dist; yarn rollup"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": ["flow", "eslint", "test"],
  "repository": {
    "type": "git",
    "url": "git@github.com:intel-hpdd/srcmap-reverse.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "@iml/flow-highland": "4.2.8",
    "@iml/flow-jasmine": "1.6.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "0.50.0",
    "highland": "3.0.0-beta.4",
    "jest": "^20.0.4",
    "lerna": "^2.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.5.3",
    "rollup": "0.45.2",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-cleanup": "1.0.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "3.0.0"
  }
}
