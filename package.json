{
  "name": "@mfl/srcmap-reverse",
  "version": "2.0.0",
  "description": "Reverses a minified stack trace using a source map.",
  "main": "source/index.js",
  "scripts": {
    "test": "jest",
    "cover": "NODE_ENV=test npm t -- --coverage",
    "flow": "flow",
    "eslint": "eslint ./"
  },
  "config": {
    "port": "8082"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/srcmap-reverse.git"
  },
  "jest": {
    "resetModules": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "test"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@mfl)/"
    ]
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "@mfl/flow-jasmine": "^1.6.0",
    "@mfl/req": "^3.0.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "flow-bin": "^0.44.2",
    "jasmine": "^2.6.0",
    "jest": "^19.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.2.2"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "highland": "^2.10.5",
    "source-map": "0.5.6"
  }
}
